{
  "manifest_version": 2,
  "name": "Color Search",
"description": "Adds a Google search card for when a color is searched. ",
  "version": "1.0.1",
"icons": { "128":"icon.png",
		"48":"icon.png","256":"icon.png"},
  "content_scripts": [
  {
    "matches": ["https://www.google.com/*"],
    "js": ["main.js"]
  }
]
}
